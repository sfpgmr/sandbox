  (local $in1 f32)
  (local $in2 f32)

  (local.set $filter
    (i32.load
      (local.get $filter_work)
    )
  )

  (local.set $a0
    (f32.load
      (i32.add 
        (&FilterWork.a0;)
        (local.get $filter_work)
      )
    )
  )
  (local.set $a1
    (f32.load
